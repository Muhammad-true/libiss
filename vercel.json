{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/courier",
      "destination": "/courier.html"
    },
    {
      "source": "/api",
      "destination": "/api.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

